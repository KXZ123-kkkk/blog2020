{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-vue-jsx", "transform-runtime",
	["prismjs", {
	      "languages": ["c","python","html", "css", "javascript", "php", "dart", "bash", "nginx", "sql"],
	      "plugins": ["line-numbers"],
	      "theme": "twilight",
	      "css": true
	    }]]
}
